
@{var model1 = Model.Last();}


@model IEnumerable<SNPData.Answer>

@{
    ViewBag.Title = "Create";
}

<h2>Create</h2>


@using (Html.BeginForm())
{
    @Html.AntiForgeryToken()

    <div class="form-horizontal">
        <h4>Answer</h4>
        <hr />
        @Html.ValidationSummary(true)

        <div class="form-group">
            @Html.LabelFor(model => model1.QuestionAnsID, "Question", new { @class = "control-label col-md-2" })
            <div class="col-md-10">
                @Html.EditorFor(model => model1.QuestionAnsID)
                @Html.ValidationMessageFor(model => model1.QuestionAnsID)
            </div>
        </div>

        <div class="form-group">
            @Html.LabelFor(model => model1.AnswerText, new { @class = "control-label col-md-2" })
            <div class="col-md-10">
                @Html.EditorFor(model => model1.AnswerText)
                @Html.ValidationMessageFor(model => model1.AnswerText)
            </div>
        </div>

        <div class="form-group">
            @Html.LabelFor(model => model1.IsActive, new { @class = "control-label col-md-2" })
            <div class="col-md-10">
                @Html.EditorFor(model => model1.IsActive)
                @Html.ValidationMessageFor(model => model1.IsActive)
            </div>
        </div>

        <div class="form-group">
            @Html.LabelFor(model => model1.Like, new { @class = "control-label col-md-2" })
            <div class="col-md-10">
                @Html.EditorFor(model => model1.Like)
                @Html.ValidationMessageFor(model => model1.Like)
            </div>
        </div>

        <div class="form-group">
            <div class="col-md-offset-2 col-md-10">
                <input type="submit" value="Create" class="btn btn-default" />
            </div>
        </div>
    </div>
}

<div>
    @Html.ActionLink("Back to List", "Index")
</div>

@section Scripts {
    @Scripts.Render("~/bundles/jqueryval")
}
